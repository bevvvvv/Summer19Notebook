% Create the function to approximate the derivative

% Create the h values and find the actual value
h = linspace(0.0001,0.1);
h = flip(h);
vals = (1 ./ (2 .* h)).*(3 .* tan(1) + 4 .* tan(1 + h) - tan(1 + 2.*h));
act = (sec(1))^2;
fl
disp(h)
disp(vals)
errors = act - vals;

figure
grid on
loglog(h, errors)







